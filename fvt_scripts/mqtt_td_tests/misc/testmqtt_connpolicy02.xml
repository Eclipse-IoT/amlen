<!-- Test that MQTT protocol must be available on Endpoint policy
  -->
<IsmWSTest name="testmqtt_connpolicy02" >
    <Action name="CreateConnection" type="CreateConnection" rc="1" reason="ISMTEST2502">
        <ActionParameter name="structure_id">Con1</ActionParameter>
        <ActionParameter name="connection_id">CF1</ActionParameter>
        <include>../common/ConnectionType.xml</include>
        <include>../common/MQTT_server.xml</include>
        <ApiParameter name="clientId">connpolicy02.mqttClient</ApiParameter>
        <ApiParameter name="port">21458</ApiParameter>
        <ApiParameter name="protocol">mqtt</ApiParameter>
        <ApiParameter name="path">config.ism.ibm.com</ApiParameter>
        <!-- ApiParameter name="verbose">true</ApiParameter-->
    </Action>
</IsmWSTest>
                