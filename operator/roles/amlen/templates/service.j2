kind: Service
apiVersion: v1
metadata:
  name: "{{ _amlen_name }}"
  namespace: "{{ _amlen_namespace }}"
  labels:
    app: "{{ _amlen_name }}"
spec:
  selector:
    app: "{{ _amlen_name }}"
  ports:
{% for key, value in amlen_service_ports.items() %}
  - port: {{value}}
    protocol: TCP
    name: "{{key}}"
    targetPort: {{value}}
{% endfor %}

  sessionAffinity: None
  type: ClusterIP
