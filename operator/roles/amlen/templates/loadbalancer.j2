apiVersion: v1
kind: Service
metadata:
  name: "{{pair_name}}-mqtt"
  namespace: "{{ _amlen_namespace }}"
spec:
  type: LoadBalancer
  externalTrafficPolicy: Cluster
  selector:
    app: "{{ app_name }}"
  ports:
{% for key, value in amlen_messaging_ports.items() %}
    - port: {{value}}
      protocol: TCP
      name: "{{key }}"
{% endfor %}
