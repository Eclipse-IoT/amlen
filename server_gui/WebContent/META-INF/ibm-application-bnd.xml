<application-bnd
    xmlns="http://websphere.ibm.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://websphere.ibm.com/xml/ns/javaee http://websphere.ibm.com/xml/ns/javaee/ibm-application-bnd_1_0.xsd"
    version="1.0">

    <security-role name="User">
        <group name="Users" />
    </security-role>
    <security-role name="SystemAdministrator">
        <group name="SystemAdministrators" />
        <group name="root" />     
    </security-role>
    <security-role name="MessagingAdministrator">
        <group name="MessagingAdministrators" />
    </security-role>
</application-bnd>